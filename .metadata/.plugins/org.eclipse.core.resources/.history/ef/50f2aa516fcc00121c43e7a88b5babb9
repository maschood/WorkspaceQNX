#ifndef PUCKTYPES_H_
#define PUCKTYPES_H_

enum puckTypes {
	PUCK_ACCEPTED,
	PUCK_TOO_LOW,
	PUCK_METAL,
	PUCK_TURNOVER,
	PUCK_HANDOVER,
};


#endif /* PUCKTYPES_H_ */
