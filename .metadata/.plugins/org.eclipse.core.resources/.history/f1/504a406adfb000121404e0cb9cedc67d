#include "HAL_ISR.h"

HAL_ISR::HAL_ISR() {

}

HAL_ISR::~HAL_ISR() {

}

void HAL_ISR::initInterrupt(){
	chid = ChannelCreate(0);

	if (chid == -1){
		cout<< "Fehler" << endl;
	}



}


int HAL_ISR::getChid(){
	return chid;
}

int HAL_ISR::getCoid(){
	return coid;
}
