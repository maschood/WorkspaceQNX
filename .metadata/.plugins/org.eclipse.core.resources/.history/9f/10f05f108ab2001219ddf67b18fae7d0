#include <stdio.h>
#include"Versuchsordner/ChildClass.h"

ChildClass::ChildClass(int id){
	id = id;
	count = count;
}
void ChildClass::shutdown(){
	cout << "Shutting down..." <<;
}

void ChildClass::ausgabe(){
	printf("ID: %d | COUNT: %d",id,count);
	count++;
}
