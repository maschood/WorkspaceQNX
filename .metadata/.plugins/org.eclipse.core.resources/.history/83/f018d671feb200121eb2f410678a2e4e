#include <stdio.h>
#include"Versuchsordner/ChildClass.h"
namespace thread{

void ChildClass::InstanzialteChildClass(int id){
	id = id;
	count = 0;
}
void ChildClass::shutdown(){
	printf("shutting down");
}

void ChildClass::ausgabe(){
	printf("ID: %d | COUNT: %d",id,count);
	count++;
}

void ChildClass::execute(void* arg){
	while(!isStopped()){
		ausgabe();
	}
}
}
