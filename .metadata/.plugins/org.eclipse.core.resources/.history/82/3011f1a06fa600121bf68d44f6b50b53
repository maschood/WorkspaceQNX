#include "RS232_2.h"

RS232_2::RS232_2() {
	if((fd = open(DEV_SER2, O_RDWR)) < 0)
	{
		printf("opening devfile2 failed\n");
		exit(EXIT_FAILURE);
	} else {
		printf("opening devfile2 SUCCESSED\n");
	}
}


